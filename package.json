    "cheerio": "^1.0.0",
    "dotenv": "^16.0.3",
    "langchain": "^0.3.19",
    "pdfjs-dist": "^4.10.38",
    "turndown": "^7.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
